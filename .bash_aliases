alias grep='rg -i'
alias la='ls -la'
alias lf='ls -la | grep'
alias ff='find | grep'
alias hisf='history | grep'
alias rmf='sudo rm -rf'
alias chgep-1='sudo chown -R $USER'
alias chgep-2='sudo chgrp -R $USER'
alias ~~='pushd /mnt/Data'
alias bashreload='source ~/.bashrc'
alias aptf='apt list | grep'
alias aptfi='apt list --installed | grep'
alias apti='sudo apt install -qy'
alias aptr='sudo apt remove -qy'
alias aptu='sudo apt autoremove -y && sudo apt full-upgrade -y'
alias fman='sudo fman'
alias vim='nvim'
alias v='nvim'
